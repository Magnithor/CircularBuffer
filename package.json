{
  "name": "mthb-circular-buffer",
  "version": "0.0.1",
  "description": "Circular buffer / ring buffer with overflow support",
  "main": "lib/CircularBuffer.js",
  "tupes": "lib/CircularBuffer.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "respository": {
    "type": "git",
    "url": "https://github.com/Magnithor/CircularBuffer.git"
  },
  "keywords": [
    "CircularBuffer",
    "RingBuffer"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": "Magni Þór Birgisson",
  "license": "ISC",
  "homepage": "https://github.com/Magnithor/CircularBuffer/blob/master/README.md",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  }
}
